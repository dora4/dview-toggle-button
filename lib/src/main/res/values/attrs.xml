        <attr name="dora_enableShadow" format="boolean" />
        <attr name="dora_shadowRadius" format="dimension|reference" />
        <attr name="dora_shadowColor" format="color|reference" />
        <attr name="dora_buttonColor" format="color|reference" />
        <attr name="dora_showIndicator" format="boolean" />
        <attr name="dora_backgroundColor" format="color|reference" />
