        <attr name="dora_uncheckColor" format="color|reference" />
        <attr name="dora_checkedColor" format="color|reference" />
        <attr name="dora_checkLineColor" format="color|reference" />
        <attr name="dora_checkLineWidth" format="dimension|reference" />
        <attr name="dora_uncheckCircleColor" format="color|reference" />
        <attr name="dora_uncheckCircleWidth" format="dimension|reference" />
        <attr name="dora_uncheckCircleRadius" format="dimension|reference" />
        <attr name="dora_borderWidth" format="dimension|reference" />
